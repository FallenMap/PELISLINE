                  <div class="col">
                     <h2><%=review.encabezado%></h2>
                  </div>
                  <div class="col">
                     <p><%=review.puntuacion%>/5</p>
                  </div>
               </div>
               <div class="row row-cols-1">
                  <div class="col">
                     <p><%=review.contenido%></p>
                  </div>
